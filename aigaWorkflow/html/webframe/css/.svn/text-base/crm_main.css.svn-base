/* update by shandl  , 1126 */
.ac_results {
	padding: 0px;
	border: 1px solid black;
	background-color: white;
	overflow: hidden;
	z-index: 99999;
}

.ac_results ul {
	width: 100%;
	list-style-position: outside;
	list-style: none;
	padding: 0;
	margin: 0;
}

.ac_results li {
	text-align:left;
	margin: 0px;
	cursor: default;
	display: block;
	/* 
	if width will be 100% horizontal scrollbar will apear 
	when scroll mode will be used
	*/
	/*width: 100%;*/
	font: menu;
	font-size: 12px;
	/* 
	it is very important, if line-height not setted or setted 
	in relative units scroll will be broken in firefox
	*/
	line-height: 16px;
	overflow: hidden;
}

.ac_loading {
	background: white url('indicator.gif') right center no-repeat;
}

.ac_odd {
	background-color: #eee;
}

.ac_over {
	background-color:#278ABF;
	color:#FFF;
}

.ac_results li a{
	padding: 2px 5px;
	cursor: default;
    display: block;
	background:#F6F6F6;
	color:#000;
	text-decoration:none;
}
.ac_results li a:hover{
	background:#278ABF;
	color:#FFF;
}
.ac_results li.ac_over a{
	background:#DDD;
	color:#000;
}

/* filterLayer 20111219 poptipmsg */
.filterLayer { position:absolute; width:100%; height:100%; top:0; left:0; z-index:12000; background:black;
 -moz-opacity:0.5; opacity:.50; filter:alpha(opacity=50);}
.filterFrame { position:absolute; width:100%; height:100%; top:0; left:0; z-index:12001; background:transparent;}
.filterFrame iframe { width:360px; height:160px; margin:0 auto; background:#F00; z-index:-1; margin-top:260px;}
.filterShadow { position:absolute; width:100%; height:100%; top:260px; left:0; z-index:12002; background:transparent;}
.filterShadow div { width:360px; height:160px; background:#666; margin:0 auto;}
.filterInner { position:absolute; width:100%; height:100%; top:260px; left:0; z-index:12003; background:transparent;}
.filterContent { width:358px; height:160px; text-align:left; background:#FFF; margin:0 auto; border:1px #3374A0 solid;}
.filterTitle { width:358px; height:28px; line-height:28px; font-weight:bold; color:#FFF; text-indent:10px;
float:left; font-size:12px; background:url(../image/crm_main_new/addLayerBg01.gif) repeat-x left top;}
.filterText { width:350px; height:60px; float:left; text-align:left; margin-left:5px; _margin-left:2px; margin-top:24px;}
.filterText tr .td01 { width:80px; height:60px; background:#FFF url(../image/crm_main_new/addLayerBg02.gif) no-repeat center 2px;}
.filterText tr .td02 { width:270px; height:60px; font-size:14px;}
.filterText tr .td02 a { color:#E00000; text-decoration:underline;font-size:14px;}
.filterText tr .td02 a:hover { color:#000; text-decoration:underline;}
