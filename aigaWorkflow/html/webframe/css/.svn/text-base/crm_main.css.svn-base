/*  yuan */
.ddsmoothmenu{
font: 12px Verdana;
width: 100%;
height:33px;
}

.ddsmoothmenu ul{
z-index:100;
margin: 0;
padding: 0;
list-style-type: none;
margin-left:4px;
}

.ddsmoothmenu ul li{
position: relative;
display: inline;
float: left;
height:33px;
}
.ddsmoothmenu ul li a{
display: block;
text-decoration: none;
}
.ddsmoothmenu ul li a:hover{
color: white;
}
.ddsmoothmenu ul li ul li a:hover{
color: black;
}

.ddsmoothmenu ul li ul{
position: absolute;
left: 0;
display: none; /*collapse all sub menus to begin with*/
visibility: hidden;
line-height:22px;
border:1px #AAA solid;
}

.ddsmoothmenu ul li ul li{
display: list-item;
float: none;
height:22px;
text-align:left;
}

.ddsmoothmenu ul li ul li ul{
top: 0;
}

.ddsmoothmenu ul li ul li a{
font: 12px Verdana;
width: 160px;
padding: 5px;
background: #EEE;
color: black;

margin: 0;
border-top-width: 0;

}


.rightarrowclass{
position: absolute;
top: 3px;
right: 5px;
}


