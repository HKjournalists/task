<?xml version="1.0" encoding="GB2312"?>
<sysbolist>
    <sysbo datasource="UserDataSource" name="BOSysParam" mainattr="PARAM_TYPE">
        <mapingenty type="table">ALM_SYS_PARAM</mapingenty>
        <datafilter />
        <attrlist>
            <attr floatlength="0" name="PARAM_TYPE" maxlength="128" datatype="String" type="COL" remark="参数类型">
                <mapingcol datatype="VARCHAR2">PARAM_TYPE</mapingcol>
            </attr>
            <attr floatlength="0" name="PARAM_ID" maxlength="12" datatype="Long" type="PK" remark="参数ID">
                <mapingcol datatype="NUMBER">PARAM_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="PARAM_NO" maxlength="128" datatype="String" type="COL" remark="参数编号">
                <mapingcol datatype="VARCHAR2">PARAM_NO</mapingcol>
            </attr>
            <attr floatlength="0" name="PARAM_NAME" maxlength="128" datatype="String" type="COL" remark="参数名称">
                <mapingcol datatype="VARCHAR2">PARAM_NAME</mapingcol>
            </attr>
            <attr floatlength="0" name="PARAM_DISC" maxlength="128" datatype="String" type="COL" remark="参数描述">
                <mapingcol datatype="VARCHAR2">PARAM_DISC</mapingcol>
            </attr>
            <attr floatlength="0" name="PARAM_RELA_ID" maxlength="12" datatype="Long" type="COL" remark="默认为 父关系 PARAM_ID">
                <mapingcol datatype="NUMBER">PARAM_RELA_ID</mapingcol>
            </attr>
            <attr floatlength="0" name="PARAM_VALUE" maxlength="4" datatype="Integer" type="COL" remark="参数值">
                <mapingcol datatype="NUMBER">PARAM_VALUE</mapingcol>
            </attr>
            <attr floatlength="0" name="SORT_ID" maxlength="4" datatype="Integer" type="COL" remark="参数排序，以sort_id的值的大小作为排次序。缺省值为0">
                <mapingcol datatype="NUMBER">SORT_ID</mapingcol>
            </attr>
        </attrlist>
        <relationlist />
        <oplist />
    </sysbo>
</sysbolist>
